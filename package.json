{
  "name": "kitapi",
  "version": "0.3.5",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-release": "react-native run-android --mode=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test:unit": "jest --testPathPattern=src/test",
    "test:e2e": "detox test --configuration android.att.debug",
    "lint": "eslint .",
    "ncu": "ncu",
    "clean": "cd android && ./gradlew clean",
    "build": "cd android && ./gradlew bundleRelease && open app/build/outputs/bundle/release",
    "deps:clean": "rimraf package-lock.json node_modules"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "jotai": "^2.0.3",
    "native-base": "3.4.28",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.71.5",
    "react-native-pager-view": "^6.2.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.8.0",
    "react-native-tab-view": "^3.5.1",
    "react-native-vector-icons": "^9.2.0",
    "styled-components": "^5.3.9",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.21.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-jest": "^29.5.0",
    "detox": "^20.6.0",
    "eslint": "8.37.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "^0.76.0",
    "npm-check-updates": "^16.9.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "rimraf": "^4.4.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
